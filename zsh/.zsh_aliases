# zsh aliases
alias python="python3"
alias t="tmux"
alias f="fzf"
alias u="sudo apt update && sudo apt -y upgrade && sudo apt autoclean && sudo apt -y autoremove && sudo snap refresh && sudo apt -y dist-upgrade"
alias update="sudo apt update && sudo apt upgrade"
alias cvim="vim /home/aldebaran/.config/nvim/init.vim"
alias calacritty="vim /home/aldebaran/.config/alacritty/alacritty.yml"
alias czsh="vim /home/aldebaran/.zshrc"
alias copy="xclip -sel c <"
alias dozip="7z a -tzip -mm=deflate -mfb=258 -mpass=15"
alias do7z="7z a -t7z -m0=lzma -mx=9 -mfb=64 -md=32m -ms=on"
alias drybkp='~/.local/src/backup.sh dry'
alias dobkp='~/.local/src/backup.sh do'
alias calc='libreoffice --calc'

alias pixemu="~/Android/Sdk/emulator/emulator -avd Pixel_API_33"

alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias lt='ls -lsth'
